{
    "name": "eol-semicolon-cr",
    "displayName": "eol-semicolon-cr",
    "description": "Insert semicolon with a keyboard shortcut and enter into a new line",
    "version": "0.0.1",
    "publisher": "psrdotcom",
    "author": "Suresh Raju Pilli",
    "license": "SEE LICENSE IN LICENSE",
    "homepage": "https://github.com/psrdotcom/eol-semicolon-cr-vscode-extn",
    "repository": {
        "type": "git",
        "url": "https://github.com/psrdotcom/eol-semicolon-cr-vscode-extn.git"
    },
    "bugs": {
        "url": "https://github.com/psrdotcom/eol-semicolon-cr-vscode-extn/issues"
    },
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "semicolon",
        "insert semicolon",
        "auto",
        "auto insert",
        "end of line"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.eol-semicolon-cr",
                "title": "Insert Semicolon",
                "category": "eol-semicolon-cr"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+;",
                "command": "extension.eol-semicolon-cr"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc  -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^8.10.25",
        "@types/vscode": "^1.34.0",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5"
    }
}
